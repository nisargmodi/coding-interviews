import java.util.ArrayList;
import java.util.List;


public class Test {
	public static void main(String[] args) {
		
		
		int count = 0;
		String wordToBeGuessed = null;
		int underscore_counter = 100;
		List<String> state_list = new ArrayList<String>();
		state_list.add("____");
		state_list.add("___");
		state_list.add("____");
		state_list.add("____");
		for (String word : state_list) { //guess the word with the least unknown letters
			for (int i = 0; i < word.length(); i++) {
				if (word.charAt(i) == '_') {
					//System.out.println(underscore_counter+"  =here");
					underscore_counter--;
				}
			}
			if (underscore_counter > count) {
				wordToBeGuessed = word;
				count = underscore_counter;
				underscore_counter = 100;
				//previousWordGuessed = word;
			}
			
			/*else if(underscore_counter == count) {
				if(previousWordGuessed.length()>word.length()) {
					wordToBeGuessed = previousWordGuessed;
					count = underscore_counter;
					underscore_counter = 100;
					previousWordGuessed = previousWordGuessed;
				}
				else {
					wordToBeGuessed = word;
					count = underscore_counter;
					underscore_counter = 1000;
					previousWordGuessed = word;
				}
			}*/
		}
		System.out.println("Word to be guessed:"+wordToBeGuessed);
		
	}
}
