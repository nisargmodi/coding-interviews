package com.practice.g2g;

public class JavaPatternMatching {
	public static void main(String[] args) {
		String s1 = "abckkkkkde";
		String s2 = "abc3kkkkkde";
		
		int count = 1;
		for(int i= 0 ; i<s1.length(); i++) {
			char s = s1.charAt(i);
			System.out.println(Character.isDigit(s));
			
		}
	}
}
