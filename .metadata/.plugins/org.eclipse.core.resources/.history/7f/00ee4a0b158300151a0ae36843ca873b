package com.practice.linkedin.glassdoor;

public class ImplementOwnMathPowerFunction {

	/*basic approach
	 * 
	 int pow(a,b) {
	 	if(a == 0) return 0;
		if(b == 0) return 1;

		if(a == 1) return 1;
		if(b == 1) return a;

		int ans = 1;
		while(b>0) {
			ans = ans*a;
			b--;
		}
	return ans;
	}
	 */
	
}
