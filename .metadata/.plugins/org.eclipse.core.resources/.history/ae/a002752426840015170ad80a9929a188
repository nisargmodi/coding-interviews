package com.practice.linkedin.glassdoor;

public class CountOfElementInSortedArray {
	int count = 0;
	public static void main(String[] args) {
		int[] a = {1,1,2,2,2,3,4,5,6,6,7,8,9,9,9};
		int key = 9;
		System.out.println(count(a, key));
	}
	
	public static int count(int[] a, int key) {
		int pos = binarySearch(a, 0, a.length-1, key);
		if(pos!= -1) {
			while (left!=) {
				int left = binarySearch(a, 0, a.length-1, key);
				int right = binarySearch(a, 0, a.length-1, key);
			}
			
		}
		return 0;
	}
	
}
