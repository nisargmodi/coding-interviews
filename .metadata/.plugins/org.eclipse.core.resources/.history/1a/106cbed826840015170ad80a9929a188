		int key = 9;
		System.out.println(count(a, key));
	}
	
	public static int count(int[] a, int key) {
		return binarySearchCount(a, 0, a.length-1, key);
	}

	private static int binarySearchCount(int[] a, int i, int j, int key) {
		
		return 0;
	}
	
}
