package com.practice.linkedin.glassdoor;

public class MirrorTree {
	public static void main(String[] args) {
		Node n = new Node(0, null, null);
		
		n = n.buildTree();
		n.print(n);
		
		Node root = mirror(n);
		root.print(root);
		
	}

	private static Node mirror(Node n) {
		Node root = n;
		if(n==null) return null;
		
		return root;
	}
}
